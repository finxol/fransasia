{{#contentFor 'title'}}
    <title>fransasia</title>
    <style>
        body div#body h1 {
            margin-top: 8rem;
        }

        div#body section {
            display: flex;
            flex-direction: row;
            justify-content: center;
            width: 100%;
        }

        div#body div {
            width: 65%;
            height: 70vh;
            background-image: url("/images/not-found.svg");
            background-size: contain;
            background-repeat: no-repeat;
            background-position: center center;
        }

        div#body div::after {
            content: "{{message}}";
            position: relative;
            top: 20vh;
            left: 9.3vw;
            font-size: 2.5rem;
            color: #ff8349;
        }
    </style>
{{/contentFor}}

<section>
    <div></div>
    <!--<h1>{{message}}</h1>-->
</section>
<h2>{{error.status}}</h2>
